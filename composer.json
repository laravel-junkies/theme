{
  "name": "laravel-junkies/theme",
  "description": "A Laravel package for multiple theme handling",
  "keywords": [
    "laravel",
    "framework",
    "theme",
    "TobyMaxham",
    "Laravel Junkies"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Tobias Maxham",
      "email": "git2020@maxham.de"
    }
  ],
  "require": {
    "php": "^7.2.5",
    "illuminate/console": "^7.0",
    "illuminate/contracts": "^7.0",
    "illuminate/view": "^7.0"
  },
  "require-dev": {
    "mockery/mockery": "^1.3.1",
    "phpunit/phpunit": "^8.4|^9.0"
  },
  "autoload": {
    "psr-4": {
      "LaraJunkie\\Theme\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "LaraJunkie\\Tests\\Theme\\": "tests/"
    }
  },
  "config": {
    "preferred-install": "dist"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "laravel": {
      "providers": [
        "LaraJunkie\\Theme\\ThemeServiceprovider"
      ],
      "aliases": {
        "Theme": "LaraJunkie\\Theme\\Facades\\LaraThemeFacade"
      }
    }
  }
}